<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.musicqapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="MusicQApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MusicQApp">
        <activity android:name="com.example.musicqapp.AlbumDetails"></activity>
        <activity android:name="com.example.musicqapp.AlbumActivity" />
        <activity android:name="com.example.musicqapp.QueueActivity" />
        <activity android:name="com.example.musicqapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>